--工程名字
PROJECT_NAME = "bson"

--目标名字
TARGET_NAME = "bson"

----工程类型: static/dynamic/exe
PROJECT_TYPE = "dynamic"

--需要的FLAGS
FLAGS = {
	"Wno-sign-compare"
}

--需要的include目录
INCLUDES = {
	"../lua/lua"
}

--需要连接的库文件
LIBS = {
	"lua"
}

--WINDOWS需要连接的库文件
WINDOWS_LIBS = {
	"ws2_32.lib"
}

--依赖项目
DEPS = {
	"lualib"
}

--分组定义
GROUP = "luaext"
